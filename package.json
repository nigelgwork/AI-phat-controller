{
  "name": "ai-controller",
  "version": "0.6.8",
  "description": "AI Controller - Multi-agent orchestration dashboard",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "tsc && vite build && electron-builder",
    "build:frontend": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build:go": "bash scripts/build-go-tools.sh",
    "preview": "vite preview",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "dist": "npm run build:go && npm run build && electron-builder --win",
    "dist:win": "npm run build:go && npm run build && electron-builder --win --publish never"
  },
  "dependencies": {
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.9",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.90.16",
    "@types/node": "^20.17.12",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "@xyflow/react": "^12.10.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.18.0",
    "lucide-react": "^0.562.0",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "wait-on": "^8.0.2",
    "zustand": "^5.0.9"
  },
  "build": {
    "appId": "com.aicontroller.app",
    "productName": "AI Controller",
    "directories": {
      "output": "release"
    },
    "extraResources": [
      {
        "from": "resources/bin",
        "to": "bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "resources/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": {
      "provider": "github",
      "owner": "nigelgwork",
      "repo": "AI-fat-controller"
    }
  }
}
